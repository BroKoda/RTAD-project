{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atonomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "overrides": [
    ],
    "parserOptions": {
